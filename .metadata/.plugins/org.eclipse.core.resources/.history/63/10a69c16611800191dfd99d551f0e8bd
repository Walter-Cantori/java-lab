package br.com.javaCollections.trainning;

import java.util.ArrayList;

public class TestandoListas {
	public static void main(String[] args) {
		String aula1 = "foo";
		String aula2 = "bar";
		String aula3 = "baz";
		
		ArrayList<String> aulas = new ArrayList<>();
		aulas.add(aula1);
		aulas.add(aula2);
		aulas.add(aula3);
	}
}
